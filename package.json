{
  "name": "bolt-app-template",
  "version": "2.5",
  "description": "A basic template to spin up a Bolt app for Slack",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-json-modules app.js",
    "dev": "nodemon --experimental-json-modules app.js",
    "test": "jest"
  },
  "dependencies": {
    "@babel/preset-env": "^7.26.0",
    "@google-cloud/translate": "^8.5.0",
    "@slack/bolt": "^4.1.0",
    "auto-bind": "^5.0.1",
    "babel-jest": "^29.7.0",
    "body-parser": "^1.20.3",
    "core-js": "^3.39.0",
    "dotenv": "^16.4.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "firebase": "^11.0.2",
    "jest": "^29.7.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.7",
    "stripe": "^17.4.0"
  },
  "engines": {
    "node": "19.x"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "verbose": true,
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ]
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/bolt-app-template"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "devDependencies": {
    "regenerator-runtime": "^0.14.1"
  }
}
